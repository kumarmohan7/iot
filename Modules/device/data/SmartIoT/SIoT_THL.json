        {
            "name": "Temperature",
            "description": " ",
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "Temperature" }
                }
            ]
        },
        {
            "name": "Humidity",
            "description": " ",
            "processList": [
                {
                    "process": "log_to_feed",
